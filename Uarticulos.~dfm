  object sqlProveedor: TMyQuery
    Connection = DMAccesos.conexion
    SQL.Strings = (
      'Select idprov,'
      
        'if(rsocial is NULL or rsocial='#39' '#39', if(apaterno is NULL or apater' +
        'no='#39' '#39', nombre, if(amaterno is NULL or amaterno='#39#39', '
      
        'concat(nombre, '#39' '#39', apaterno), concat(nombre, '#39' '#39', apaterno, '#39' '#39 +
        ', amaterno))), rsocial) as NombreCompleto'
      ''
      'from proveedores'
      ''
      'order by NombreCompleto')
    FetchAll = True
    Left = 497
    Top = 469
    object sqlProveedoridprov: TIntegerField
      FieldName = 'idprov'
      Origin = 'proveedores.idprov'
    end
    object sqlProveedorNombreCompleto: TStringField
      FieldName = 'NombreCompleto'
      Origin = 'proveedores.NombreCompleto'
      Size = 102
    end
  end
