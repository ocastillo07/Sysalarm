  object sqlgeneral: TMyQuery
    Connection = DMAccesos.conexion
    SQL.Strings = (
      'select idcliente, idpedido, saldo '
      ''
      'from pedidos'
      ''
      'where Saldo < '#39'5'#39' and not Saldo = '#39'0'#39' '
      ''
      'and idcliente='#39'505'#39
      ''
      'UNION'
      ''
      'select idcliente, idpedido, restante as saldo '
      ''
      'from PagosClientes '
      ''
      
        'where restante < '#39'5'#39'  and not restante='#39'0'#39' and ( movimiento = '#39'0' +
        #39' or movimiento = '#39'1'#39')  '
      ''
      'and idcliente='#39'505'#39
      ''
      'order by idcliente')
    FetchAll = True
